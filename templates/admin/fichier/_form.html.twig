<div class="container mt-15">
    {{form_start(form)}}
    <div class="row">
        <div class="col-3">{{form_row(form.titre)}}</div>
        <div class="col-2">{{form_row(form.type)}}</div>
        <div class="col-2">{{form_row(form.specialite)}}</div>
                <div class="col-2">{{form_row(form.module)}}</div>

        <div class="col-1">{{form_row(form.semestre)}}</div>
        <div class="col-2">{{form_row(form.annee)}}</div>
    </div>
    <div class="row">
        <div class="col-4 ">{{ form_row(form.pdfFile) }}</div>
    </div>
    {{ form_rest(form) }}
    <button class="btn btn-primary"><i class="fas fa-upload" style="padding-right:10px;"></i>{{
        button|default('Enregistrer') }}</button>
    {{ form_end(form) }}
</div>